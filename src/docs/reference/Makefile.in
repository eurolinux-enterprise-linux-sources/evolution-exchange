	-cp $(srcdir)/tmpl/*.sgml $(distdir)/tmpl
	-cp $(srcdir)/xml/*.xml $(distdir)/xml
	-cp $(srcdir)/html/* $(distdir)/html

.PHONY : dist-hook-local

dist-hook-local:
	mkdir $(distdir)/features
	-cp $(srcdir)/features/*.xml $(distdir)/features

-include $(top_srcdir)/git.mk

